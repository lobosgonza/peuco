<body class="bg-fondo">
  
  
  <nav class=" navbar navbar-expand-lg navbar-light bg-dark">

   
 <a class="navbar-brand" href="#"> <h1>Peuco</h1></a>

      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>
  <div class="collapse navbar-collapse" id="navbarNavAltMarkup"></div>
     <div class="navbar-nav">
    <ul class="navbar-nav">
      <li class="nav-item active"> <a class="nav-link" href="#home">Home</a></li>
      <li class="nav-item"> <a class="nav-link" href="#nosotros">Nosotros</a></li>
      <li class="nav-item"> <a class="nav-link" href="#productos">Productos</a></li>
      <li class="nav-item"> <a class="nav-link" href="#contacto">Contacto</a></li>
    
        
        
         
    </ul>

    </div>
  </nav>


  <div class="container-body bgImgPortada">

    <div class="container-body-title ">
      <h1>Peuco</h1>

      <a class="button scroll" href="#productos">Productos</a>
    </div>

  </div>

  <div class="  container-body container-body-queHacemos">
    <div class="container ">


      <div class="row">
        <div class="col-md-6">
          <img src="" class="d-block w-100 product-img" alt="Imagen 1">
        </div>
        <div class="col-md-6">
     <hr class="line">
          <h3>Amor por la madera</h3>
          <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam, quis nostrud exercitation ullamco </p>
     <hr class="line">
        </div>

      </div>
    </div>
  </div>


  <div class=" container-body scroll" id="productos">
    <div class="container">

      <h2>Nuestros Productos</h2>

      <div id="carouselExampleFade" class="carousel slide carousel-fade" data-ride="carousel">
        <div class="carousel-inner">
          <% items.map((item)=>{ %>
                   
            
            <div class="carousel-item <%= item.active %> >">
              <div class="row">
                <div class="offset-2 col-8 col-md-4">
                  <img src="<%= item.img %>" class="d-block w-100 product-img" alt="<%= item.nombre %>">
                 
                </div>
                <div class="offset-md-0 offset-2 col-8 col-md-4">
                  <p> <%= item.nombre %></p>
                  <p><%= item.descripcion %></p>
                   <p><%= item.categoria %></p>
                    <p><%= item.dimensiones %></p>
                     <p><%= item.precio %></p>
                    
                  </div>
                </div>
              </div>
              
              <% }) %>
              
              
              
       
        </div>
        <a class="carousel-control-prev" href="#carouselExampleFade" role="button" data-slide="prev">
          <span class="carousel-control-prev-icon" aria-hidden="true"></span>
          <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselExampleFade" role="button" data-slide="next">
          <span class="carousel-control-next-icon" aria-hidden="true"></span>
          <span class="sr-only">Next</span>
        </a>
      </div>
    </div>

  </div>


  <footer>
    <div class=" footer-container">
      <div class="container">


        <div class="row">
          <div class="col text-center">
            <p>Direccion</p>
            <p>Numero de telefono</p>
            <p>maildecontacto@peuco.cl</p>
          </div>
         

        </div>
      </div>
    </div>


    <div class="footer-copyright text-center">
      <p>Peuco 2020</p>
    </div>
  </footer>
</body>


